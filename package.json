{
  "name": "@zkochan/remark-toc",
  "version": "0.1.0",
  "description": "Generate a Table of Contents (TOC) for Markdown files",
  "license": "MIT",
  "keywords": [
    "text",
    "markdown",
    "plain",
    "text",
    "table",
    "of",
    "contents",
    "content",
    "toc",
    "generation",
    "remark"
  ],
  "dependencies": {
    "remark-slug": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/remark-toc.git"
  },
  "bugs": "https://github.com/wooorm/remark-toc/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "@zkochan/remark": "^0.1.0",
    "browserify": "^13.0.0",
    "eslint": "^2.0.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.4.0",
    "jscs": "^2.0.0",
    "jscs-jsdoc": "^1.0.0",
    "remark-comment-config": "^3.0.0-alpha.1",
    "remark-github": "^4.0.1",
    "remark-lint": "^2.0.0",
    "remark-usage": "^2.1.0",
    "remark-validate-links": "^2.0.0",
    "tape": "^4.0.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-bundle": "browserify index.js --no-builtins -s remarkTOC > remark-toc.js",
    "build-mangle": "esmangle remark-toc.js > remark-toc.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint-api": "eslint .",
    "lint-style": "jscs --reporter inline .",
    "lint": "npm run lint-api && npm run lint-style",
    "test-api": "node test/index.js",
    "test-coverage": "istanbul cover test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  }
}
